{
    "name": "radebatz/silex2swagger",
    "description": "Generate swagger documentation from Silex Annotations",
    "license": [
        "MIT"
    ],
    "homepage": "http://radebatz.net/mano/",
    "authors": [
        {
            "name": "Martin Rademacher",
            "email": "mano@radebatz.org"
        }
    ],
    "support": {
        "issues": "https://github.com/DerManoMann/silex2swagger/issues",
        "source": "https://github.com/DerManoMann/silex2swagger.git"
    },
    "require": {
        "php": ">=5.5.9",
        "ddesrosiers/silex-annotation-provider": "~2.0",
        "zircote/swagger-php": "~2.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "symfony/console": "~2.8|3.0.0",
        "phpunit/phpunit": "~5.0",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Radebatz\\Silex2Swagger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Radebatz\\Silex2Swagger\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/silex2swagger"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
